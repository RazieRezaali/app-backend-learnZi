        "/api/user/register": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Register a new user",
                "description": "This endpoint allows you to register a new user.",
                "operationId": "register",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "fname",
                                    "lname",
                                    "email",
                                    "password",
                                    "password_confirmation",
                                    "phone",
                                    "age",
                                    "country_id",
                                    "level_id"
                                ],
                                "properties": {
                                    "fname": {
                                        "type": "string",
                                        "example": "Saul"
                                    },
                                    "lname": {
                                        "type": "string",
                                        "example": "Goodman"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "saul@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "Pass123!"
                                    },
                                    "password_confirmation": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "Pass123!"
                                    },
                                    "phone": {
                                        "type": "string",
                                        "example": "+989123456789"
                                    },
                                    "age": {
                                        "type": "integer",
                                        "example": 32
                                    },
                                    "country_id": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "level_id": {
                                        "type": "integer",
                                        "example": 3
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "User registered successfully."
                                        },
                                        "user": {
                                            "properties": {
                                                "id": {
                                                    "type": "integer",
                                                    "example": 1
                                                },
                                                "fname": {
                                                    "type": "string",
                                                    "example": "Saul"
                                                },
                                                "lname": {
                                                    "type": "string",
                                                    "example": "Goodman"
                                                },
                                                "email": {
                                                    "type": "string",
                                                    "example": "saul@example.com"
                                                },
                                                "phone": {
                                                    "type": "string",
                                                    "example": "09123456789"
                                                },
                                                "age": {
                                                    "type": "integer",
                                                    "example": 32
                                                },
                                                "country_id": {
                                                    "type": "integer",
                                                    "example": 1
                                                },
                                                "level_id": {
                                                    "type": "integer",
                                                    "example": 3
                                                },
                                                "created_at": {
                                                    "type": "string",
                                                    "format": "date-time",
                                                    "example": "2025-04-10T10:00:00Z"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthenticated",
